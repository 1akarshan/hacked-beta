ePage',\n    components: {\n      IonContent,\n      IonThumbnail,\n      IonPage,\n      IonCard,\n      IonCardContent,\n      IonCardTitle,\n      IonCardSubtitle\n\n    },\n    data() {\n      return {\n        items: [\n          {title: 'Question 1', body: 'Easy', question: 'What does the fox say? Answer the question ', color: 'success'},\n          {title: 'Question 2', body: 'Easy', question: 'What does the fox say? Answer the question ', color: 'success'},\n          {title: 'Question 3', body: 'Hard', question: 'What does the fox say? Answer the question ', color: 'danger'},\n          {title: 'Question 4', body: 'Medium', question: 'What does the fox say? Answer the question ', color: 'warning'},\n          {title: 'Question 5', body: 'Easy', question: 'What does the fox say? Answer the question ', color: 'success'},\n          {title: 'Question 6', body: 'Hard', question: 'What does the fox say? Answer the question ', color: 'warning'},\n          {title: 'Question 7', body: 'Medium', question: 'What does the fox say? Answer the question ', color: 'warning'},\n          {title: 'Question 8', body: 'Hard', question: 'What does the fox say? Answer the question ', color: 'danger'},\n          {title: 'Question 9', body: 'Hard', question: 'What does the fox say? Answer the question ', color: 'danger'},\n          {title: 'Question 10', body: 'Easy', question: 'What does the fox say? Answer the question ', color: 'success'}\n          \n        ]\n      }\n    },\n    methods: {\n      async openModal(id, title, body) {\n        const modal = await modalController.create({\n          component: Modal,\n          props: {\n            id : id,\n            title : title,\n            body : body\n          }\n        });\n      modal.present();\n      \n      }\n    }\n  })\n</script>\n\n<style scoped>\nion-content{ \n  font-family: 'Roboto', sans-serif !important;\n  font-size:2em;\n}\n.box {\n  width:100vw;\n  height:50vh;\n  display:inline-block;\n  padding